{
  "name": "Inflearn Extend",
  "version": "1.2.0",
  "description": "Inflearn Utility",
  "permissions": ["storage", "tabs"],
  "host_permissions": ["https://www.inflearn.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.inflearn.com/*"],
      "js": ["index.js"]
    }
  ],
  "manifest_version": 3,
  "action": {
    "default_title": "Inflearn Extend",
    "default_popup": "popup.html"
  }
}
